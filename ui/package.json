    "ts-node": "10.9.2",
    "tsc-watch": "6.3.1",
    "typescript": "5.7.3",
    "vite": "5.4.19",
    "vitest": "1.6.1"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "resolutions": {
    "fork-ts-checker-webpack-plugin": "^8.0.0"
  }
}
