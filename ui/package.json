    "@testing-library/react": "13.4.0",
    "@types/command-line-args": "5.2.3",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.111",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "arg": "5.0.2",
    "chromatic": "11.25.2",
    "css-loader": "6.11.0",
    "dev-kong": "0.11.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "9.1.0",
