{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js packages/*",
    "fix": "eslint --fix --ext .ts,.tsx,.js packages/*",
    "type-check": "tsc --noEmit",
    "test": "jest --coverage --config jest.config.cjs",
    "build": "lerna run build",
    "watch": "lerna run --parallel watch",
    "watch-parca-dev": "lerna run --parallel --include-dependencies --scope @parca/web watch",
    "publish:ci": "lerna publish --yes --no-verify-access",
    "benchmark": "NODE_ENV=production BROWSERSLIST='Chrome > 100' GENERATE_SOURCEMAP=false ts-node --experimental-specifier-resolution=node scripts/run-benchmark.mts",
    "prettier": "prettier --write --ignore-path ../.prettierignore 'packages/**/*.{js,jsx,ts,tsx,md,mdx}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic"
  },
  "workspaces": {
    "packages": [
      "packages/app/*",
      "packages/shared/*"
    ],
    "nohoist": [
      "**/d3",
      "**/jest",
      "**/@grafana/toolkit"
    ]
  },
  "dependencies": {
    "@parca/react-benchmark": "^5.3.0",
    "command-line-args": "^5.2.1",
    "date-fns": "2.30.0",
    "not-a-log": "^1.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/preset-env": "^7.20.2",
    "@ianvs/prettier-plugin-sort-imports": "3.7.2",
    "@mdx-js/loader": "2.3.0",
    "@next/bundle-analyzer": "12.3.4",
    "@next/eslint-plugin-next": "12.3.4",
    "@next/mdx": "12.3.4",
    "@storybook/addon-actions": "^7.0.0-rc.3",
    "@storybook/addon-docs": "7.0.23",
    "@storybook/addon-essentials": "^7.0.0-rc.3",
    "@storybook/addon-interactions": "^7.0.0-rc.3",
    "@storybook/addon-links": "^7.0.0-rc.3",
    "@storybook/addon-mdx-gfm": "^7.0.0-rc.3",
    "@storybook/addon-outline": "7.0.23",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/addon-storyshots": "7.0.23",
    "@storybook/addon-styling": "^0.3.1",
    "@storybook/node-logger": "^7.0.0-rc.3",
    "@storybook/preset-create-react-app": "^7.0.0-rc.3",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^7.0.0-rc.3",
    "@storybook/react-webpack5": "^7.0.0-rc.3",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/webpack": "6.5.1",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.66",
    "@swc/jest": "0.2.26",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/command-line-args": "^5.2.0",
    "@types/jest": "29.5.2",
    "@types/node": "18.16.18",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "arg": "5.0.2",
    "chromatic": "^6.17.2",
    "css-loader": "6.8.1",
    "dev-kong": "0.11.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-config-standard-with-typescript": "22.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "26.9.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-standard": "5.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-typescript-enum": "^2.1.0",
    "execa": "^6.1.0",
    "fs-extra": "10.1.0",
    "glob-promise": "^5.0.0",
    "globby": "13.2.0",
    "identity-obj-proxy": "3.0.0",
    "internal-ip": "7.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lerna": "6.6.2",
    "local-web-server": "5.3.0",
    "next-transpile-modules": "9.1.0",
    "ora": "^6.1.2",
    "plop": "3.1.2",
    "pm2": "5.3.0",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "react-is": "18.2.0",
    "react-test-renderer": "18.2.0",
    "replace-in-files": "3.0.0",
    "rimraf": "3.0.2",
    "sass": "1.63.6",
    "sass-loader": "13.3.2",
    "storybook": "^7.0.0-rc.3",
    "storybook-dark-mode": "^2.1.1",
    "style-loader": "3.3.3",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "tsc-watch": "6.0.4",
    "typescript": "4.9.5",
    "webpack": "5.88.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
